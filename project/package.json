{
  "name": "vant-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@saeris/vue-spinners": "^1.0.8",
    "amfe-flexible": "^2.2.1",
    "animate.css": "^3.7.0",
    "anywhere": "^1.5.0",
    "axios": "^0.18.0",
    "better-scroll": "^1.13.4",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "exif-js": "^2.3.0",
    "js-cookie": "^2.2.0",
    "js-md5": "^0.7.3",
    "register-service-worker": "^1.5.2",
    "vant": "^1.4.8",
    "velocity": "^0.7.2",
    "velocity-animate": "^1.5.2",
    "vue": "^2.5.17",
    "vue-amap": "^0.5.8",
    "vue-clipboard2": "^0.3.0",
    "vue-content-placeholders": "^0.2.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "weixin-js-sdk": "1.3.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-plugin-pwa": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "autoprefixer": "^9.4.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.13.1",
    "postcss-pxtorem": "^4.0.1",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "no-case-declarations": 0,
      "block-spacing": 0,
      "no-unused-vars": 0,
      "no-duplicate-imports": 1,
      "prefer-template": 0,
      "no-var": 2,
      "no-const-assign": 2,
      "no-undef": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-sparse-arrays": 2,
      "eqeqeq": 2,
      "no-multi-str": 2,
      "prefer-promise-reject-errors": 2,
      "radix": 2,
      "require-await": 2
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "AMap": true,
      "WeixinJSBridge": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
